<ipfixcol2>
  <inputPlugins>
    <input>
      <name>IPFIX File</name>
      <plugin>ipfix</plugin>
      <params>
        <path>/home/adam/bp/run/flow_files/100M_flows.ipfix</path>
      </params>
    </input>
  </inputPlugins>

  <outputPlugins>
    <output>
      <name>IPFIX output</name>
      <plugin>ipfix</plugin>
      <params>
          <filename>output.ipfix</filename>
          <useLocalTime>false</useLocalTime>
          <windowSize>300</windowSize>
          <alignWindows>true</alignWindows>
          <preserveOriginal>false</preserveOriginal>
          <rotateOnExportTime>false</rotateOnExportTime>
      </params>
    </output>
    </outputPlugins>
</ipfixcol2>
