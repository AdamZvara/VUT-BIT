<ipfixcol2>
<!-- Input plugins -->
  <inputPlugins>
    <input>
      <name>TCP input</name>
      <plugin>tcp</plugin>
      <params>
          <localPort>8000</localPort>
      </params>
  </input>
  </inputPlugins>

  <!-- Output plugins -->
  <outputPlugins>
    <output>
      <name>Dummy output</name>
      <plugin>dummy</plugin>
      <params>
        <delay>0</delay>
        <stats>true</stats>
      </params>
    </output>
  </outputPlugins>
</ipfixcol2>